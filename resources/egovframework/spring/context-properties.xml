<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean name="propertiesService" class="egovframework.rte.fdl.property.impl.EgovPropertyServiceImpl" destroy-method="destroy">
		<property name="properties">
	        <map>
	        	<entry key="pageUnit" value="${globals.properties.pageUnit}" />
				<entry key="pageSize" value="${globals.properties.pageSize}" />
				<entry key="posblAtchFileSize" value="${globals.properties.posbAtchFileSize}" />
				<entry key="Globals.fileStorePath" value="${globals.properties.fileStorePath}" />
				<entry key="Globals.addedOptions" value="${globals.properties.addedOptions}" />
	        </map>
		</property>
	</bean>
	
</beans>	
