	<script type="text/javascript">
		$(function() {
			//확대이미지 보기
			imageZoom = function(url,title){
				$('.photo_zoom img').attr('src',url);
				$('.photo_zoom img').attr('alt',title);
				$('.photo_zoom').addClass('on');
			};
			$('.photo_list td a').click(function(){
				var iurl = $(this).find('img').attr('src');
				var ititle = $(this).find('p').text();
				imageZoom(iurl,ititle);
			});
			//확대닫기
			$('.photo_zoom a').click(function(){
				$('.photo_zoom').removeClass('on');
			});
		});
	</script>
