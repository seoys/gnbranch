<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE HTML>
<html lang="ko">
<head>
	<meta charset="UTF-8">
	<title>지부 어드민</title>
	<link rel="stylesheet" type="text/css" href="/css/mgr/base.css" />
</head>
<body>
	<div class="login-box">
		<h1><img src="/images/mgr/common/h1_logo.gif" alt="굿네이버스" /></h1>
		<h2>Admin</h2>
		<fieldset>
			<legend>로그인</legend>
			<div class="login-form">
				<input type="text" name="" id="" title="아이디 입력창" class="form-control"/>
				<input type="password" name="" id="" title="비밀번호 입력창" class="form-control"/>
				<!-- label for="" & input id="" 매칭 -->
				<p class="check"><input type="checkbox" name="" id="" /> <label for="">아이디 저장</label></p>
				<button class="btn btn-green">Login</button>
			</div>
		</fieldset>
	</div>
</body>
</html>